  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/SergiuToporjinschi/node-red-contrib-heater-controller/issues"
  },
  "files": [
    "nodes/**/*.*"
  ],


  "nyc": {
    "exclude": [
      "spec/**"
    ],
    "reporter": [
      "lcov",
      "html"
    ]
  },
  "mocha": {
    "diff": true,
    "extension": [
      "js"
    ],
    "slow": 1500,
    "timeout": 20000,
    "recursive": true,
    "spec": [
      "spec/*.spec.js"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "lodash": "~4.17.20",
    "mocha": "^8.2.1",
    "mocha-param": "^2.0.1",
    "nodemon": "*",
    "nyc": "^15.1.0",
    "should": "^8.4.0",
    "should-sinon": "*",
    "sinon": "^9.2.4"
  }
